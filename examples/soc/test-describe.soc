// Test describe function

let data = {
    name: "John Doe",
    age: 25,
    scores: [95, 87, 92, 88],
    address: {
        street: "123 Main St",
        city: "Boston",
        zip: "02101"
    },
    active: true
}

print("Describing complex data structure:")
print(describe(data))

print("\nDescribing just the scores:")
print(describe(data.scores))

print("\nDescribing a function:")
let double = x => x * 2
print(describe(double))
