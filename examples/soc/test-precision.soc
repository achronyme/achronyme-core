// Test precision configuration system

print("=== Default precision (full precision) ===")
print(0.33333333333)
print(1.0 + 0.0000000001)
print(3.14159265358979323846)

// Test complex numbers - note: e^(i*pi) + 1 would require complex exp
print(0 + 3i)
print(0 + 1i)
print(0 - 1i)
print(5 + 0i)
print(2 + 3i)
print(2 - 3i)

print("\n=== Precision 2 decimal places ===")
set_precision(2)
print(0.33333333333)
print(1.0 + 0.0000000001)
print(3.14159265358979323846)
print(1.666666667)
print(2.71828182845904523536)

print("\n=== Complex with precision 2 ===")
print(0 + 3i)
print(0 + 1i)
print(0 - 1i)
print(5.123 + 0.00001i)
print(2.7777 + 3.1111i)
print(2.5555 - 3.9999i)
print(0.001 + 0.001i)

print("\n=== Integer precision (precision 0) ===")
set_precision(0)
print(3.7)
print(2.1 + 2.8)
print(9.999)
print(0.4999)

print("\n=== Complex with integer precision ===")
print(2.7 + 3.9i)
print(0.1 + 1i)
print(5.5 + 0.001i)

print("\n=== Precision 4 decimal places ===")
set_precision(4)
print(3.141592653589793)
print(2.718281828459045)
print(1.414213562373095)

print("\n=== Epsilon test: very small numbers ===")
set_precision(2)
print(0.0000000001)
print(1.0 + 0.0000000001)
print(0.0000000001 + 0.0000000001i)

print("\n=== Reset to full precision ===")
set_precision(-1)
print(3.141592653589793)
print(2.718281828459045)
print(1.0 / 3.0)

print("\n=== Vector and record formatting with precision ===")
set_precision(2)
print([1.111, 2.222, 3.333, 4.444])
print([0.33333, 0.66666, 0.99999])
