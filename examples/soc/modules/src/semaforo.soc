let semaforo = {
    mut luz: "Verde",
    next: () => do{
        match self.luz {
            "Verde" => do { self.luz = "Rojo" },
            "Rojo" => do { self.luz = "Amarillo" },
            "Amarillo" => do { self.luz = "Verde" },
            _ => do { self.luz = "Verde" },
        }
        self.luz
    }
}

export { semaforo }