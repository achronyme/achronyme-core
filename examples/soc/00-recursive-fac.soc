let factorial = (n: Number): Number => do{
    if (n <= 1) {
        1
    } else {
        n * rec(n - 1)
    }
}

let fibbonacci = (n: Number): Number => do{
    if (n <= 1) {
        n
    } else {
        rec(n - 1) + rec(n - 2)
    }
}

print("Factorial of 5 is:", factorial(5))  // Expected output: 120
print("Factorial of 20 is:", factorial(10))
print("Fibbonacci of 10 is:", factorial(1000) - 1)