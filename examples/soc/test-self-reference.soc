// Test self reference in record methods

let counter = {
    mut value: 0,
    inc: () => do {
        self.value = self.value + 1
    },
    dec: () => do {
        self.value = self.value - 1
    },
    getValue: () => self.value
}

// Test it
counter.inc()
counter.inc()
print(counter.getValue())  // Should print 2

counter.dec()
print(counter.getValue())  // Should print 1
