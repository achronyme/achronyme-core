// Test default parameter values in lambdas

// Test 1: Basic default parameters
let defecto = (x: Number, y: Number = 100, z: Number = 200) => do {
    x + y + z
}

print("Test 1: Default parameters")
print("  With all args:", defecto(50, 60, 70))  // Expected: 180
print("  With 2 args:", defecto(50, 60))         // Expected: 250 (50+60+200)
print("  With 1 arg:", defecto(50))              // Expected: 350 (50+100+200)

// Test 2: String default parameters
let greet = (name, greeting = "Hello") => greeting + ", " + name

print("\nTest 2: String defaults")
print("  Custom greeting:", greet("Alice", "Hi"))  // Expected: "Hi, Alice"
print("  Default greeting:", greet("Bob"))         // Expected: "Hello, Bob"

// Test 3: Default parameters with expressions
let addNumbers = (a, b = a * 2, c = b + 10) => a + b + c

print("\nTest 3: Expression-based defaults")
print("  With 1 arg:", addNumbers(5))       // Expected: 5 + 10 + 20 = 35
print("  With 2 args:", addNumbers(5, 3))   // Expected: 5 + 3 + 13 = 21
print("  With 3 args:", addNumbers(5, 3, 1)) // Expected: 5 + 3 + 1 = 9

// Test 4: All optional parameters
let allOptional = (x = 10, y = 20, z = 30) => x + y + z

print("\nTest 4: All optional parameters")
print("  No args:", allOptional())           // Expected: 60
print("  1 arg:", allOptional(1))            // Expected: 51
print("  2 args:", allOptional(1, 2))        // Expected: 33
print("  3 args:", allOptional(1, 2, 3))     // Expected: 6

// Test 5: Mixed required and optional
let mixed = (required, optional1 = 100, optional2 = 200) => required + optional1 + optional2

print("\nTest 5: Mixed required and optional")
print("  Required only:", mixed(5))          // Expected: 305
print("  Required + 1 optional:", mixed(5, 10))  // Expected: 215
print("  All args:", mixed(5, 10, 20))       // Expected: 35
